algoritmo "Qual é maior"
var
   valor1,valor2:real
inicio
   escreval("Olá, informe o primeiro valor: ")
   leia(valor1)
   
   escreval("Olá, informe o segundo valor: ")
   leia(valor2)

   se valor1>valor2 então
   escreval ("O primeiro valor é o maior número")
   senao
        escreval ("O segundo valor é o maior número")
   fimse
fimalgoritmo